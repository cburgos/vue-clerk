{
  "name": "vue-clerk",
  "version": "0.0.12",
  "packageManager": "pnpm@8.10.2",
  "description": "",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/vue-clerk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-clerk.git"
  },
  "bugs": "https://github.com/wobsoriano/vue-clerk/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch --onSuccess \"pnpm build:dts && pnpm --filter playground dev\"",
    "build": "tsup --onSuccess \"pnpm build:dts\"",
    "build:dts": "vue-tsc --declaration --emitDeclarationOnly",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@clerk/clerk-js": "^4.64.2",
    "@clerk/types": "^3.57.1",
    "@vueuse/core": "^10.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "@testing-library/vue": "^8.0.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "changelogen": "^0.5.5",
    "eslint": "^8.52.0",
    "eslint-plugin-import": "^2.29.0",
    "jsdom": " ^22.1.0",
    "taze": "^0.12.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "unplugin-vue": "^4.3.5",
    "vite": "^4.5.0",
    "vitepress": "1.0.0-rc.24",
    "vitest": "^0.34.6",
    "vue": "^3.3.7",
    "vue-tsc": "^1.8.22"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "n/prefer-global/process": "off"
    }
  },
  "eslintIgnore": [
    ".vitepress"
  ]
}
