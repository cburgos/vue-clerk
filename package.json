{
  "name": "vue-clerk",
  "version": "0.1.2",
  "packageManager": "pnpm@8.14.3",
  "description": "",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/vue-clerk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-clerk.git"
  },
  "bugs": "https://github.com/wobsoriano/vue-clerk/issues",
  "keywords": [
    "auth",
    "clerk"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js"
    },
    "./headless": {
      "types": "./dist/headless.d.ts",
      "import": "./dist/headless.mjs",
      "require": "./dist/headless.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "headless.d.ts",
    "plugin.d.ts"
  ],
  "scripts": {
    "dev": "tsup --watch --onSuccess \"pnpm build:dts && pnpm --filter playground dev\"",
    "build": "tsup --onSuccess \"pnpm build:dts\"",
    "build:dts": "vue-tsc --declaration --emitDeclarationOnly",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@clerk/clerk-js": "^4.68.5",
    "@clerk/types": "^3.60.0",
    "@vueuse/core": "^10.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@testing-library/jest-dom": "^6.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "jsdom": "^24.0.0",
    "taze": "^0.13.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "unplugin-vue": "^5.0.2",
    "vite": "^4.5.2",
    "vitepress": "1.0.0-rc.24",
    "vitest": "^1.2.1",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}